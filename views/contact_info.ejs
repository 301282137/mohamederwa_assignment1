<!-- Header -->
<%- include ('partials/header.ejs'); %>

<!-- Main Nav-->
<%- include ('partials/main_nav.ejs'); %>

<!-- Main Content -->
<main class="container">
  <div class="row">
    <div class="col-md-offset-3 col-md-8">
      <h1><%= title %></h1>
      <br />

      <table class="table table-bordered table-striped table-hover">
        <!-- Header Row -->
        <tr>
          <th class="text-center">Name</th>
          <th class="text-center">Number</th>
          <th class="text-center">Email</th>
        </tr>

        <!-- Repeatable Template Row -->
        <% for(let count=0; count < ContactList.length; count++) { %>
        <tr>
          <td class="text-center"><%= ContactList[count].contactName %></td>

          <td class="text-center"><%= ContactList[count].contactNumber %></td>

          <td class="text-center"><%= ContactList[count].email %></td>
        </tr>
        <% } %>
      </table>
    </div>
  </div>
</main>

<!-- Bottom Nav-->
<%- include ('partials/bottom_nav.ejs'); %>

<!-- footer -->
<%- include ('partials/footer.ejs'); %>
